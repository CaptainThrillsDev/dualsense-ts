

class InputState {
  left: Analog;
  right: Analog;

  // Right analog
  RX: number;
  RY: number;
  R3: boolean;

  dPadState: number;

  R1: boolean;
  R2: number;
  L1: boolean;
  L2: number;

  triangle: boolean;
  circle: boolean;
  cross: boolean;
  square: boolean;

  options: boolean;
  share: boolean;
  R2Btn: boolean;
  L2Btn: boolean;
  ps: boolean;
  touchBtn: boolean;
  micBtn: boolean;
}
